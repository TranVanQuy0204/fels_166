<%= form_for [:admin, @category] do |f| %>
  <%= render "/shared/error_messages", object: f.object %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="form-group pull-right">
    <%= f.submit t(".save"), class: "btn btn-primary" %>
  </div>
<% end %>
